{
  "name": "rn-vant",
  "version": "0.2.1",
  "description": "React Native UI Components",
  "keywords": [
    "react-native",
    "Components"
  ],
  "license": "MIT",
  "author": "bijinfeng <bijinfeng@bytedance.com>",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "source": "src/index",
  "react-native": "src/index",
  "types": "lib/typescript/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "src",
    "docs"
  ],
  "scripts": {
    "bootstrap": "yarn --cwd example && yarn",
    "build": "bob build",
    "build:doc": "dumi build",
    "build:example": "yarn --cwd example build:web",
    "commit": "git cz",
    "doc": "dumi dev",
    "generate:icon": "npx iconfont-rn",
    "lint": "eslint --ext '.js,.ts,.tsx' . --fix",
    "lint-staged": "lint-staged --allow-empty",
    "prepare": "husky install",
    "test": "jest"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@callstack/react-theme-provider": "^3.0.8",
    "@gorhom/portal": "^1.0.14",
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-picker/picker": "^2.4.8",
    "color": "^4.2.3",
    "expo-image-picker": "^14.1.0",
    "lodash-es": "^4.17.21",
    "react-hook-form": "^7.43.0",
    "react-native-animatable": "^1.3.3",
    "react-native-drop-shadow": "^0.0.6",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-popover-view": "^5.1.7",
    "react-native-svg": "^13.8.0"
  },
  "devDependencies": {
    "@callstack/eslint-config": "^13.0.2",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@testing-library/react-native": "^11.5.1",
    "@types/color": "^3.0.3",
    "@types/jest": "^26.0.23",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.27",
    "@types/react-native": "^0.71.2",
    "@types/react-test-renderer": "^18.0.0",
    "@youngjuning/eslint-config": "^1.4.0",
    "@youngjuning/prettier-config": "^1.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-import": "^1.13.3",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.0.2",
    "dumi": "^2.1.7",
    "dumi-theme-mobile": "^2.0.3",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "expo-modules-core": "^0.11.5",
    "husky": "^8.0.3",
    "jest": "^26.6.3",
    "jest-expo": "^46.0.1",
    "lerna": "5.4.3",
    "lint-staged": "^13.1.0",
    "metro-react-native-babel-preset": "^0.72.1",
    "prettier": "^2.8.3",
    "react": "^18.2.0",
    "react-art": "^18.2.0",
    "react-native": "^0.71.2",
    "react-native-builder-bob": "^0.20.3",
    "react-native-iconfont-cli": "^2.2.4",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-svg-web": "^1.0.9",
    "react-native-web": "^0.18.12",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-safe-area-context": "*"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.json"
        }
      ]
    ]
  }
}
